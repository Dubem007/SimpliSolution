{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=localhost\\SQLEXPRESS;Database=SimplifiedUIDB;Trusted_Connection=True;TrustServerCertificate=True",
    "RedisCon": "redis:6379,allowAdmin=true"
  },
  "JwtSettings": {
    "ValidIssuer": "SimplifiedUI",
    "ValidAudience": "*",
    "ExpiresIn": "7200",
    "Secret": "someRandom-16Digit-Keys",
    "TokenLifespan": 7
  },
  "MsgQueue": {
    "DelayInMilliseconds": "10000",
    "IsAutoAcknowledged": true
  },
  "QueueSettings": {
    "QueueConfig": {
      "QueueName": "SimplifiedUISvc_Queue",
      "QueueConString": "amqp://guest:guest@localhost:15672/"
    }
  }
}
